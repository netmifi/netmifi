{
    "env": {
      "browser": true,
      "es2016": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "commonjs"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      // Add custom rules here, for example:
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "off"
    }
  }
  